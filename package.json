{
  "name": "valorant-tips-frontend",
  "homepage": "http://gabrielogregorio.github.io/valorant-tips",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "jest": "jest",
    "test": "jest && yarn run cypress:headless",
    "jest:coverage": "jest --coverage",
    "jest:watchAll": "jest --watchAll",
    "jest:watchAll:coverage": "jest --watchAll --coverage",
    "cypress": "cross-env NODE_ENV=test cypress open",
    "cypress:headless": "cross-env NODE_ENV=test cypress run --headless --browser chrome",
    "deprecated:e2e:headless": "next build && start-server-and-test start http://localhost:3000 cypress:headless",
    "type-check": "tsc",
    "lint": "next lint",
    "stylelint": "stylelint '**.css'",
    "check-all": "tsc && next lint && stylelint '**.css' && jest --coverage && jest-coverage-badges && yarn cypress:headless && yarn audit --groups 'dependencies'",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public",
    "predeploy": "yarn run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "deploy-storybook-gh-pages": "yarn run build-storybook && yarn run deploy-storybook"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@loadable/component": "^5.15.2",
    "@next/bundle-analyzer": "^12.2.3",
    "axios": "^0.26.0",
    "identity-obj-proxy": "^3.0.0",
    "msw": "0.39.2",
    "next": "^12.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.28.1",
    "react-icons": "^4.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-query": "^3.34.16",
    "sharp": "^0.30.5",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/code-coverage": "^3.9.12",
    "@react-mock/localstorage": "^0.1.2",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/chai": "^4.1.7",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.2",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-cypress": "^0.28.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-storybook": "^0.5.11",
    "eslint-plugin-testing-library": "^5.3.1",
    "express": "^4.18.0",
    "gh-pages": "^3.2.3",
    "http": "^0.0.1-security",
    "jest": "^27.5.1",
    "jest-coverage-badges": "^1.1.2",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.22",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "4.6.2",
    "webpack": "^5.69.1"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier"
    ],
    "rules": {
      "length-zero-no-unit": true,
      "block-no-empty": true,
      "declaration-no-important": true,
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "tailwind",
            "apply",
            "variants",
            "responsive",
            "screen"
          ]
        }
      ]
    }
  }
}
